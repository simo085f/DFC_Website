    <kontaktOs id="1278" key="fbc85db8-9daf-4614-9dbf-d0a32be1d58d" parentID="1066" level="2" creatorID="0" sortOrder="4" createDate="2019-12-07T13:49:56" updateDate="2019-12-08T20:41:21" nodeName="Kontakt os" urlName="kontakt-os" path="-1,1066,1278" isDoc="" nodeType="1079" creatorName="Simone Grauer" writerName="Simone Grauer" writerID="0" template="1078" nodeTypeAlias="kontaktOs" isPublished="true">
